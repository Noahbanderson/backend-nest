{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Repl: nestjs",
			"port": 9229,
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Api: nestjs",
			"remoteRoot": "/home/src/backend",
			"localRoot": "${workspaceFolder}",
			"protocol": "inspector",
			"trace": true,
			"restart": true,
			"skipFiles": ["<node_internals>/**"]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Worker: nestjs",
			"remoteRoot": "/home/src/backend",
			"localRoot": "${workspaceFolder}",
			"protocol": "inspector",
			"trace": true,
			"restart": true,
			"skipFiles": ["<node_internals>/**"],
			"port": 9339
		}
	]
}
