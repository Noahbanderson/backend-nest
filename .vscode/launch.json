{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug: nestjs - api",
      "remoteRoot": "/src/app",
      "localRoot": "${workspaceFolder}",
      "protocol": "inspector",
      "restart": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Debug: nestjs - worker",
      "remoteRoot": "/src/app",
      "localRoot": "${workspaceFolder}",
      "protocol": "inspector",
      "restart": true,
      "skipFiles": ["<node_internals>/**"],
      "port": 9339
    }
  ]
 }